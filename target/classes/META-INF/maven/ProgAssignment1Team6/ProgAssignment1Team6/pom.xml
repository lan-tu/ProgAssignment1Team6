	<modelVersion>4.0.0</modelVersion>
	<groupId>ProgAssignment1Team6</groupId>
	<artifactId>ProgAssignment1Team6</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Team6 - Progmming Assignment 1</name>
